{
  "productName": "electron-updater-private",
  "appId": "com.quasartec.quasar-desktop",
  "directories": {
    "output": "build/${version}"
  },
  "publish": [
    {
      "provider": "github",
      "private": true,
      "owner": "QuasarTec",
      "repo": "https://github.com/QuasarTec/QuasarMessenger/tree/master/QuasarMessenger-desktop",
      "token": "c35947029da582c837642aff169622be718084e0"
    }
  ],
  "files": [
    "dist/electron/**/*"
  ],
  "dmg": {
    "contents": [
      {
        "x": 410,
        "y": 150,
        "type": "link",
        "path": "/Applications"
      },
      {
        "x": 130,
        "y": 150,
        "type": "file"
      }
    ]
  },
  "mac": {
    "icon": "build/icons/icon.icns"
  },
  "win": {
    "icon": "build/icons/icon.ico"
  },
  "linux": {
    "icon": "build/icons"
  }
}